{
  "name": "beam-for-individuals-api",
  "version": "1.0.0",
  "description": "BEAM for Individuals - AI Search Visibility Platform API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "test:load": "artillery run load-test.yml",
    "test:security": "npm audit && snyk test",
    "db:init": "node db-init.js",
    "db:migrate": "node migrations/run-migrations.js",
    "db:seed": "node seeds/seed-data.js",
    "db:backup": "node scripts/backup-database.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "logs": "tail -f /var/log/beam/app.log"
  },
  "keywords": [
    "beam",
    "ai-search",
    "competitive-intelligence",
    "local-business"
  ],
  "author": "BEAM Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "mysql2": "^3.6.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "axios": "^1.6.2",
    "stripe": "^14.0.0",
    "node-cron": "^3.0.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "artillery": "^2.0.0",
    "snyk": "^1.1277.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
